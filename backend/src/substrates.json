{
  "CPRG": {
    "metric": "R/G ratio",
    "expression": "r / g",
    "thresholds": {
      "negative": "value < 1.5",
      "positive": "value > 2.0",
      "moderate": "1.5 <= value <= 2.0"
    }
  },
  "Nitrocefin": {
    "metric": "Hue Angle",
    "expression": "hue_angle(r, g, b)",
    "thresholds": {
      "negative": "value > 35",
      "positive": "value < 25",
      "moderate": "25 <= value <= 35"
    }
  },
  "Resazurin": {
    "metric": "Hue Angle",
    "expression": "hue_angle(r, g, b)",
    "thresholds": {
      "negative": "240 <= value < 290",
      "positive": "value >= 310 or value <= 10",
      "moderate": "290 <= value < 310"
    }
  },
  "Au NP - R hue": {
    "metric": "R component",
    "expression": "r",
    "thresholds": {
      "negative": "value > 115",
      "positive": "value < 110",
      "moderate": "110 <= value <= 115"
    }
  }
}

{
  "CPRG": {
    "metric": "R/G ratio",
    "expression": "r / g",
    "thresholds": {
      "negative": "value < 1.5",
      "positive": "value > 2.0"
    }
  },
  "Nitrocefin": {
    "metric": "Hue Angle",
    "expression": "60 * (((g-b)/(r-b)) % 6)",
    "thresholds": {
      "negative": "value > 35",
      "positive": "value < 25"
    }
  }
}
